{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"label": "Build test",
			"command": "/usr/bin/gcc",
			"args": [
				"-g",
				"-o",
				"${workspaceFolder}/test",
				"${workspaceFolder}/ssmp.c",
				"${workspaceFolder}/Test/test.c"
			],
			"options": {
				"cwd": "/usr/bin"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		}
	]
}